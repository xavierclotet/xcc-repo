{
  "version": 1,
  "projects": {
    "xcc": {
      "projectType": "application",
      "root": "apps/xcc",
      "sourceRoot": "apps/xcc/src",
      "prefix": "xcc",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/xcc",
            "index": "apps/xcc/src/index.html",
            "main": "apps/xcc/src/main.ts",
            "polyfills": "apps/xcc/src/polyfills.ts",
            "tsConfig": "apps/xcc/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/xcc/src/favicon.ico", "apps/xcc/src/assets"],
            "styles": ["apps/xcc/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/xcc/src/environments/environment.ts",
                  "with": "apps/xcc/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "xcc:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "xcc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "xcc:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/xcc/src/**/*.ts",
              "apps/xcc/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/xcc"],
          "options": {
            "jestConfig": "apps/xcc/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "xcc-e2e": {
      "root": "apps/xcc-e2e",
      "sourceRoot": "apps/xcc-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/xcc-e2e/cypress.json",
            "tsConfig": "apps/xcc-e2e/tsconfig.e2e.json",
            "devServerTarget": "xcc:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "xcc:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/xcc-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui-toolbar": {
      "projectType": "library",
      "root": "libs/ui/toolbar",
      "sourceRoot": "libs/ui/toolbar/src",
      "prefix": "xcc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/toolbar/src/**/*.ts",
              "libs/ui/toolbar/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/toolbar"],
          "options": {
            "jestConfig": "libs/ui/toolbar/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-test": {
      "projectType": "library",
      "root": "libs/ui/test",
      "sourceRoot": "libs/ui/test/src",
      "prefix": "xcc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/test/src/**/*.ts",
              "libs/ui/test/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/test"],
          "options": {
            "jestConfig": "libs/ui/test/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
